<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tedu.coolshark.mapper.UserMapper">
    <select id="selectByUsername" resultType="cn.tedu.coolshark.pojo.vo.UserVo">
        select password,nickname from user where username=#{username}
    </select>
</mapper>
